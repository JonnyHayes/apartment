{% extends 'base.twig' %}

  {% block content %}

  {% spaceless %}

  <!-- Styles for floating nav -->
  {% include 'css/floating-nav-active-colors.twig' %}
  <style>
  form {font-family: "Gotham A","Gotham B";}
  @media screen and (min-width: 992px) {
    .col-md-3 {
        width: 23%;
    }
  }
  .warning {display:block;}

  </style>
  <!-- Floating nav -->
  <div class="floating-nav-container">
    <ul id="sales-process-nav-ul">
      <li class="floating-nav active"><a href="#sales-process">Sales Process</a></li>
      <li class="floating-nav"><a href="#step-by-step">Step By Step</a></li>
      <li class="floating-nav"><a href="#workshop-video">Workshop Videos</a></li>
      <li class="floating-nav"><a href="#lenders">Lenders</a></li>
      <li class="floating-nav"><a href="#rent_vs_own_calcaulator">Rent vs Own</a></li>
      <li class="floating-nav"><a href="#faq">FAQs</a></li>
    </ul>
  </div>

  <div id="sales-process-nav-wrapper">
    <!-- Sales Process heading -->
    <div id="sales-process" class="bg-image">
      <div class="container h-center">
        <div class="row">
          <div class="content-text-wrapper">
            <div class="col-sm-4 col-md-4 col-xs-12 title-h1-container">
              <div class="heading-bar-top white-text"></div>
              <h1 class="white-text" id="sales-process-title">{{ sales_process.title }}</h1>
              <div class="heading-bar-bottom white-text"></div>
            </div>
            <div class="col-sm-6 col-md-6 col-xs-12">
              <h3 class="white-text default">Reserved Housing</h3>
              <p class="white-text top-paragraph">
                {{ sales_process.get_field('sales_process_content') }}
              </p>
            </div>
          </div>
        </div>
      </div>
        <div class="row bottom-row down-arrow-container first-section">
          <div class="col-md-12 last-column">
            <div class="last-column-wrapper">
              <p class="text-center white-text italic-text">
                <a href="#step-by-step">
                Sales Process<br />
                <img src="{{function('get_stylesheet_directory_uri')}}/img/optimized/icon-next-ylw.png"
                srcset="{{function('get_stylesheet_directory_uri')}}/img/optimized/icon-next-ylw.png 1x,
                {{function('get_stylesheet_directory_uri')}}/img/optimized/icon-next-ylw@x2.png 2x" />
                </a>
              </p>
            </div>
          </div>
        </div>
    </div>

    <!-- Sales Process infographic -->
    <div id="step-by-step" class="add-vh fade-up" style="padding-top: 160px;">
      <div class="container h-center extra-tall">
        <div class="row">
          <div class="col-md-12 center-img">
            <ul class="flex-container wrap">
              <li class="flex-item">
                <img class="img-responsive" src="{{function('get_stylesheet_directory_uri')}}/img/optimized/salesprocess-steps_01.png" />
              </li>
              <li class="flex-item">
                <img class="img-responsive" src="{{function('get_stylesheet_directory_uri')}}/img/optimized/salesprocess-steps_02.png" />
